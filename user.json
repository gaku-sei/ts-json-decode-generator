{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "User",
  "type": "object",
  "properties": {
    "unknown": { "type": "null" },
    "age": { "type": "number" },
    "name": { "type": "string" },
    "gender": { "type": "string", "enum": ["female", "male"] },
    "adult": { "type": "boolean" },
    "address": {
      "type": "object",
      "properties": {
        "street": { "type": "string" },
        "country": { "type": "string" },
        "parts": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["country", "parts"]
    }
  },
  "required": ["name", "age"]
}
